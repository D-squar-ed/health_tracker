<p id="notice"><%= notice %></p>

<h1>Listing Lbs</h1>

<table>
  <thead>
    <tr>
      <th>Weight</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lbs.each do |lb| %>
      <tr>
        <td><%= lb.weight %></td>
        <td><%= lb.date %></td>
        <td><%= link_to 'Show', lb %></td>
        <td><%= link_to 'Edit', edit_lb_path(lb) %></td>
        <td><%= link_to 'Destroy', lb, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lb', new_lb_path %>
