<h1>Listing Step Numbers</h1>

<table>
  <thead>
    <tr>
      <th>Steps Taken</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @step_numbers.each do |step_number| %>
      <tr>
        <td><%= step_number.steps_taken %></td>
        <td><%= step_number.date %></td>
        <td><%= link_to 'Show', step_number %></td>
        <td><%= link_to 'Edit', edit_step_number_path(step_number) %></td>
        <td><%= link_to 'Destroy', step_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Step Number', new_step_number_path %>



